{
  "name": "react-kapsule",
  "version": "2.1.5",
  "description": "A React wrapper for Kapsule-style web components",
  "unpkg": "dist/react-kapsule.min.js",
  "main": "dist/react-kapsule.common.js",
  "module": "dist/react-kapsule.module.js",
  "repository": {
    "type": "git",
    "url": "git+https://github.com/vasturiano/react-kapsule.git"
  },
  "keywords": [
    "react",
    "wrapper",
    "kapsule",
    "web",
    "component"
  ],
  "author": {
    "name": "Vasco Asturiano",
    "url": "http://bl.ocks.org/vasturiano"
  },
  "license": "MIT",
  "bugs": {
    "url": "https://github.com/vasturiano/react-kapsule/issues"
  },
  "homepage": "https://github.com/vasturiano/react-kapsule",
  "scripts": {
    "build": "rimraf dist && rollup -c",
    "dev": "rollup -c -w",
    "minify": "terser dist/react-kapsule.js -o dist/react-kapsule.min.js -c -m --comments '/Version/'",
    "build-module": "rollup -c rollup.config.module.js",
    "prepare": "npm run build && npm run minify && npm run build-module"
  },
  "files": [
    "dist/**/*"
  ],
  "dependencies": {
    "fromentries": "^1.2.0",
    "jerrypick": "^1.0.1"
  },
  "peerDependencies": {
    "react": ">=16.8.0"
  },
  "devDependencies": {
    "@babel/core": "^7.8.7",
    "@babel/plugin-proposal-class-properties": "^7.8.3",
    "@babel/plugin-proposal-object-rest-spread": "^7.8.3",
    "@babel/preset-env": "^7.8.7",
    "@babel/preset-react": "^7.8.3",
    "@rollup/plugin-commonjs": "^11.0.2",
    "@rollup/plugin-node-resolve": "^7.1.1",
    "rimraf": "^3.0.2",
    "rollup": "^2.0.6",
    "rollup-plugin-babel": "^4.4.0",
    "terser": "^4.6.6"
  }
}
